extends layout.pug

block content
    #map
   
    script(type='text/javascript').     
        var map = L.map('map').setView([65, 28.5], 5);L.tileLayer('http://{s}.tile.osm.org/{z}/{x}/{y}.png',{attribution: '&copy; <a href="http://osm.org/copyright">OpenStreetMap</a> contributors'}).addTo(map);
        var markervals="#{values}";
        markervals=markervals.split(',');
        var longvals="#{longvalues}";
        longvals=longvals.split(',');
        var moistvals="#{moistvalues}";
        moistvals=moistvals.split(',');
        console.log(markervals[0]);
        for(i=0;i<markervals.length;i++)
        {       
        console.log(markervals[i]);
        console.log(longvals[i]);
        console.log(moistvals[i]);
        var marker = new L.marker([markervals[i], longvals[i]], { opacity: 0.01 });marker.bindLabel(moistvals[i]+"%", {noHide: true, className: "my-label", offset:[0,0]});marker.addTo(map);
        }
        
            